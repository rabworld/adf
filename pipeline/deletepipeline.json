{
	"name": "deletepipeline",
	"properties": {
		"activities": [
			{
				"name": "Delete csv",
				"type": "Delete",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "fileDeleted",
						"value": "All .txt files are deleted successfully"
					}
				],
				"typeProperties": {
					"dataset": {
						"referenceName": "Dataset2",
						"type": "DatasetReference"
					},
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						}
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": false,
						"wildcardFileName": "*.txt",
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"annotations": []
	}
}