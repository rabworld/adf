{
	"name": "deletepipeline",
	"properties": {
		"activities": [
			{
				"name": "Delete1",
				"type": "Delete",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "FilesDelete",
						"value": "successfully txt files deleted"
					}
				],
				"typeProperties": {
					"dataset": {
						"referenceName": "outputFolderDataset",
						"type": "DatasetReference"
					},
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "linkedservice_storage",
							"type": "LinkedServiceReference"
						},
						"path": "adfcontainer/output"
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true,
						"wildcardFileName": "\".txt\""
					}
				}
			}
		],
		"annotations": []
	}
}